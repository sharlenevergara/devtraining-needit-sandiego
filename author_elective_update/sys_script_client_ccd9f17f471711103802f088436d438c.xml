        <field>u_requested_for</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>NeedIt Populate Email Field</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	
	//For New Record, the newValue has a value of currently logged in user when a form loads
    if (isLoading && !g_form.isNewRecord() || newValue === '') {
        return;
    }

    var ga = new GlideAjax('GetEmailAddress'); //GetEmailAddress is the Script Include class
    ga.addParam('sysparm_name', 'getEmail'); //getEmail is the function you want to call
    ga.addParam('sysparm_user', g_form.getValue('u_requested_for')); //Send the captured value to Script Include
    ga.getXML(setRequestedForEmail); //Execute the code

    //In callback function, we can get the return value from the Script Include
    function setRequestedForEmail(response) {
        var answer = response.responseXML.documentElement.getAttribute('answer');
        g_form.setValue('u_requested_for_email', answer);
    }

}]]></script>
